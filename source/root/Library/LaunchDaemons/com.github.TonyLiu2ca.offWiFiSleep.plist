<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.github.TonyLiu2ca.offWiFiSleep</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/offWiFiSleep/sleepwatcher</string>
		<string>-V</string>
		<string>-s /Library/offWiFiSleep/.sleep</string>
		<string>-w /Library/offWiFiSleep/.wakeup</string>
	</array>
	<key>RunAtLoad</key>
	<true/>
	<key>KeepAlive</key>
	<true/>
</dict>
</plist>
